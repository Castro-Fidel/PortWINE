        kill_portwine
    elif [[ "${PW_SOUND_DRIVER_USE}" == "disabled" ]] \
    && grep -v "\"Audio\"=" "${PORT_WINE_PATH}/data/prefixes/${PW_PREFIX_NAME}/user.reg" &>/dev/null
